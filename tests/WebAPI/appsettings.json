{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "AllowedHosts": "*",

  "ConnectionStrings": {
    "localDb": "server=(local); database=TestDb; trusted_connection=true;"
  },

  "Datasets": {
    "ClientDatasetName": "Acme"
  },

  "OpenAPI": {
    "ApiName": "ProxyR API",
    "ApiDescription": "ProxyR endpoints API",
    "ApiVersion": "V1",
    "DocumentName": "endpoint",
    "UseBearerAuthentication": false,
    "UseFullNameForSchemaIds": true
  },

  "ProxyR": {
    "Prefix": "Query_",
    "Suffix": "",
    "Seperator": "_",
    "DefaultSchema": "dbo",
    "IncludeSchemaInPath": false,
    "ExcludedParameters": [
      "CurrentUserId",
      "CurrentUserName",
      "CurrentRoleId",
      "CurrentRoleName"
    ]
  }
}